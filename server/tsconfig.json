{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"baseUrl": "."
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}